{
  "expo": {
    "name": "HabitForge",
    "slug": "habitforge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Assets.xcassets/AppIcon.appiconset/1024.png",
    "scheme": "habitforge",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android/mipmap-xxxhdpi/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Assets.xcassets/AppIcon.appiconset/32.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Assets.xcassets/AppIcon.appiconset/1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
